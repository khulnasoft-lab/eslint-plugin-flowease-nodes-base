<% validInvalid.forEach( ( section ) => { %>
<%- include( section.valid ? 'exampleValid' : 'exampleInvalid', { section: section } ) %>
<%- /packageJson/.test(section.examples)
  ? section.examples
    .replace('const packageJson = ', '')
    .replace('```js', '```json')
  : section.examples
  %>
<% } ) %>

<% fixed.forEach( ( section ) => { %>
<%- include( 'exampleFixed', { section: section } ) %>
<%- section.examples %>
<% } ) %>